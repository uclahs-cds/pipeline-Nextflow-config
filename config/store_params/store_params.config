import groovy.json.JsonOutput

json_extractor {
    store_object_json = { Map object, File destination ->
        json_object = JsonOutput.prettyPrint(JsonOutput.toJson(object))
        File parent = destination.getParentFile()
        if (!parent.exists()) {
            parent.mkdirs()
            }
        file.write(json_object)
      }
 }
